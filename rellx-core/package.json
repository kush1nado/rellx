    "state-management",
    "state",
    "store",
    "reactive",
    "typescript",
    "javascript",
    "middleware",
    "plugins"
  ],
  "author": "kush1nado",
  "license": "Apache-2.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
