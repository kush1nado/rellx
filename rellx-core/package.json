{
  "type": "module",
  "name": "rellx",
  "version": "2.0.1",
  "description": "Universal state manager for JavaScript and TypeScript applications",
  "keywords": [
    "state-management",
    "state",
    "store",
    "reactive",
    "typescript",
    "javascript",
    "middleware",
    "plugins"
  ],
  "author": "kush1nado",
  "license": "Apache-2.0",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./light": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./full": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "jest": "^29.7.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1"
  },
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run type-check && npm run test && npm run build"
  },
  "dependencies": {}
}
